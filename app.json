{
  "expo": {
    "name": "Carbonara",
    "slug": "Carbonara",
    "scheme": "carbonara",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.4.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "14.0"
          },
          "android": {}
        }
      ],
      "expo-font"
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "carbonara"
    },
    "android": {
      "package": "com.auth0samples"
    },
    "description": "Carbonara food app"
  }
}
